{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "version": "4.0.0",
    "notes": "Daten k√∂nnen jetzt auch offline eingesehen werden"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/assets/images/**",
          "/assets/fonts/**",
          "/*.css",
          "/*.js",
          "/*.woff2"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://api.chucknorris.io/**"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "5d",
        "timeout": "3s",
        "strategy": "freshness"
      }
    }
  ]
}